<!DOCTYPE html>
<html lang="en" ng-app="ZNxtAppAdmin">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
      <!-- Meta, title, CSS, favicons, etc. -->
      <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
          <meta name="viewport" content="width=device-width, initial-scale=1">
            <link rel="icon" href="images/favicon.ico" type="image/ico" />

            <title>@Model["app_name"] @Model["page_title"]</title>

            <!-- Bootstrap -->
            <link href="@Model["base_uri"]/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
              <!-- Font Awesome -->
              <link href="@Model["base_uri"]/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
                <!-- NProgress -->
                <link href="@Model["base_uri"]/vendors/nprogress/nprogress.css" rel="stylesheet">
                  <!-- iCheck -->
                  <link href="@Model["base_uri"]/vendors/iCheck/skins/flat/green.css" rel="stylesheet">

                    <!-- bootstrap-progressbar -->
                    <link href="@Model["base_uri"]/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
                      <!-- JQVMap -->
                      <link href="@Model["base_uri"]/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet"/>
                      <!-- bootstrap-daterangepicker -->
                      <link href="@Model["base_uri"]/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">

                        <!-- Custom Theme Style -->
                        <link href="@Model["base_uri"]/build/css/custom.min.css" rel="stylesheet">
						
    <!-- jQuery -->
    <script src="@Model["base_uri"]/vendors/jquery/dist/jquery.min.js"></script>

						
  </head>

  <body class="nav-md" ng-controller="ZNxtAppAdmin.Main">
    <div class="container body">
      <div class="main_container">
        <div class="col-md-3 left_col">

          @Include("./header.zb",null)
         
            <!-- /menu profile quick info -->

            <br />

            <!-- sidebar menu -->

          @Include("./left_slider_menu.zb",null)
            <!-- /sidebar menu -->
          </div>
        </div>

		
        <!-- top navigation -->
		 @Include("./top_nevigation.zb",null)
        <!-- /top navigation -->

        <!-- page content -->
      @RenderBody()

      <!-- /page content -->

        <!-- footer content -->
        @Include("./footer.zb",null)
        <!-- /footer content -->
      </div>
    </div>

    <!-- Bootstrap -->
    <script src="@Model["base_uri"]/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Angular js  -->
    <script src="@Model["base_uri"]/vendors/angularjs/angular.min.js"></script>
    <!-- Angular js route  -->
    <script src="@Model["base_uri"]/vendors/angularjs/angular-route.js"></script>
     <!-- Angular js anamiation  -->
    <script src="@Model["base_uri"]/vendors/angularjs/angular-animate.js"></script>
	 <!-- Get  js -->
    <script src="@Model["base_uri"]/api/getjs?path=/backend/app"></script>
	
    <!-- FastClick -->
    <script src="@Model["base_uri"]/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="@Model["base_uri"]/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="@Model["base_uri"]/vendors/Chart.js/dist/Chart.min.js"></script>
    <!-- gauge.js -->
    <script src="@Model["base_uri"]/vendors/gauge.js/dist/gauge.min.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="@Model["base_uri"]/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- iCheck -->
    <script src="@Model["base_uri"]/vendors/iCheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="@Model["base_uri"]/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="@Model["base_uri"]/vendors/Flot/jquery.flot.js"></script>
    <script src="@Model["base_uri"]/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="@Model["base_uri"]/vendors/Flot/jquery.flot.time.js"></script>
    <script src="@Model["base_uri"]/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="@Model["base_uri"]/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="@Model["base_uri"]/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="@Model["base_uri"]/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="@Model["base_uri"]/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- DateJS -->
    <script src="@Model["base_uri"]/vendors/DateJS/build/date.js"></script>
    <!-- JQVMap -->
    <script src="@Model["base_uri"]/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="@Model["base_uri"]/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="@Model["base_uri"]/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="@Model["base_uri"]/vendors/moment/min/moment.min.js"></script>
    <script src="@Model["base_uri"]/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>

    <!-- Custom Theme Scripts -->
    <script src="@Model["base_uri"]/build/js/custom.min.js"></script>

  </body>
</html>
